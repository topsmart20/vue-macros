{
  "name": "@vue-macros/devtools",
  "version": "0.1.1",
  "packageManager": "pnpm@7.27.0",
  "description": "Devtools plugin for Vue Macros.",
  "keywords": [
    "vue-macros",
    "macros",
    "vue",
    "sfc",
    "setup",
    "script-setup",
    "devtools"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-macros#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-macros.git",
    "directory": "packages/devtools"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "dev": "./src/index.ts",
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts && vite build ./src/client",
    "dev": "DEV=true tsup",
    "client:dev": "vite ./src/client"
  },
  "peerDependencies": {
    "vite": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "sirv": "^2.0.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@unocss/reset": "^0.49.5",
    "@vitejs/plugin-vue": "^4.0.0",
    "get-port": "^6.1.2",
    "unocss": "^0.49.4",
    "vite": "^4.1.1"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
