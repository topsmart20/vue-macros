{
  "name": "@vue-macros/reactivity-transform",
  "version": "0.2.5",
  "packageManager": "pnpm@7.30.0",
  "description": "reactivity-transform feature from Vue Macros.",
  "keywords": [
    "vue-macros",
    "macros",
    "vue",
    "sfc",
    "setup",
    "script-setup",
    "reactivity-transform",
    "unplugin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-macros#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-macros.git",
    "directory": "packages/reactivity-transform"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "macros.d.ts",
    "macros-global.d.ts",
    "vue2-macros.d.ts",
    "vue2-macros-global.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": [
      "./*",
      {
        "dev": "./src/*.ts",
        "types": "./dist/*.d.ts",
        "require": "./dist/*.js",
        "import": "./dist/*.mjs"
      },
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "<=4.9": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts",
    "dev": "DEV=true tsup"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.2.25"
  },
  "dependencies": {
    "@babel/parser": "^7.21.3",
    "@vue-macros/common": "workspace:~",
    "@vue/compiler-core": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "magic-string": "^0.30.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "estree-walker": "^3.0.3"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
