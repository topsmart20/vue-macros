{
  "name": "@vue-macros/better-define",
  "version": "1.3.1",
  "packageManager": "pnpm@7.26.0",
  "description": "",
  "keywords": [
    "unplugin",
    "vue",
    "sfc",
    "setup",
    "macros",
    "script-setup",
    "better-define"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-macros#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-macros.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "dev": "./src/vite.ts",
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "dev": "./src/webpack.ts",
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "dev": "./src/rollup.ts",
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "dev": "./src/esbuild.ts",
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts",
    "dev": "DEV=1 tsup"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@vue-macros/api": "workspace:~",
    "@vue-macros/common": "workspace:~",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "rollup": "^3.14.0"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
