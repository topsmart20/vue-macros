{
  "name": "@vue-macros/setup-component",
  "version": "0.15.4",
  "packageManager": "pnpm@7.30.0",
  "description": "setup-component feature from Vue Macros.",
  "keywords": [
    "vue-macros",
    "macros",
    "vue",
    "sfc",
    "setup",
    "script-setup",
    "setup-component",
    "unplugin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-macros#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-macros.git",
    "directory": "packages/setup-component"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "macros.d.ts",
    "macros-global.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": [
      "./*",
      {
        "dev": "./src/*.ts",
        "types": "./dist/*.d.ts",
        "require": "./dist/*.js",
        "import": "./dist/*.mjs"
      },
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "<=4.9": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts",
    "dev": "DEV=true tsup"
  },
  "dependencies": {
    "@vue-macros/common": "workspace:~",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "rollup": "^3.20.0",
    "vue": "^3.2.47"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
