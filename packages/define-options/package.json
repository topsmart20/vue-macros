{
  "name": "unplugin-vue-define-options",
  "version": "0.7.0",
  "packageManager": "pnpm@7.8.0",
  "description": "Add defineOptions support for Vue <script setup>.",
  "keywords": [
    "unplugin",
    "vue",
    "script-setup",
    "defineOptions"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-define-options#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-define-options/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-define-options.git",
    "directory": "packages/define-options"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./cores": {
      "require": "./dist/cores.js",
      "import": "./dist/cores.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.37",
    "unplugin": "^0.8.0"
  },
  "devDependencies": {
    "@babel/types": "^7.18.10",
    "@vue-macros/common": "workspace:*",
    "fast-glob": "^3.2.11",
    "rollup": "^2.77.2",
    "tsup": "^6.2.1",
    "tsx": "^3.8.0",
    "unplugin-vue": "^3.0.1",
    "vite": "^3.0.4",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
