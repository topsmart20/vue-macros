{
  "name": "unplugin-vue-define-options",
  "version": "0.10.0",
  "packageManager": "pnpm@7.9.3",
  "description": "Add defineOptions support for Vue <script setup>.",
  "keywords": [
    "unplugin",
    "vue",
    "macros",
    "script-setup",
    "defineOptions"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-macros/tree/main/packages/define-options#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-macros.git",
    "directory": "packages/define-options"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "macros.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./unplugin": {
      "require": "./dist/unplugin.js",
      "import": "./dist/unplugin.mjs"
    },
    "./macros": "./macros.d.ts",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@vue-macros/common": "workspace:*",
    "ast-walker-scope": "^0.2.1",
    "unplugin": "^0.9.2"
  },
  "devDependencies": {
    "@babel/types": "^7.18.10",
    "fast-glob": "^3.2.11",
    "rollup": "^2.78.0",
    "tsup": "^6.2.2",
    "tsx": "^3.8.2",
    "unplugin-vue": "^3.0.1",
    "vite": "^3.0.8",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
