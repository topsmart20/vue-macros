{
  "name": "monorepo-unplugin-vue-macros",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "description": "Monorepo for Vue macros.",
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "scripts": {
    "build": "pnpm run -C ./packages/define-options build && pnpm run -C ./packages/macros build",
    "play": "pnpm run -C ./playground dev",
    "test": "vitest",
    "release": "bumpp package.json ./packages/*/package.json && pnpm publish --filter \"unplugin-vue*\"",
    "lint": "eslint . --ext .vue,.ts,.md,.json --max-warnings 0",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^2.3.5",
    "@types/node": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "eslint-define-config": "^1.6.0",
    "fast-glob": "^3.2.11",
    "tsup": "^6.2.1",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4",
    "vitest": "0.20.2",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
