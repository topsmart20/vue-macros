{
  "name": "monorepo-unplugin-vue-macros",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.25.1",
  "description": "Monorepo for Vue macros.",
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "scripts": {
    "build": "turbo run build --filter=\"./packages/*\"",
    "dev": "pnpm run --filter=\"./packages/*\" --parallel dev",
    "clean": "rm -fr packages/*/dist",
    "docs:dev": "pnpm run -C ./docs dev",
    "docs:serve": "pnpm run -C ./docs serve",
    "docs:build": "turbo run build --filter docs",
    "play": "pnpm run play:vue3",
    "play:vue2": "pnpm run -C ./playground/vue2 dev",
    "play:vue3": "pnpm run -C ./playground/vue3 dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "format": "prettier . \"!**/fixtures\" --write --cache",
    "publish": "pnpm build && changeset publish",
    "typecheck": "run-p \"typecheck:*\"",
    "typecheck:lib": "tsc --noEmit -p tsconfig.lib.json",
    "typecheck:test": "tsc --noEmit -p tsconfig.test.json"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@pnpm/find-workspace-packages": "^5.0.27",
    "@pnpm/logger": "^5.0.0",
    "@sxzz/eslint-config": "^2.4.4",
    "@types/node": "18.11.18",
    "@vitest/coverage-c8": "^0.28.1",
    "@vitest/ui": "^0.28.1",
    "@vue-macros/test-utils": "workspace:*",
    "eslint": "^8.32.0",
    "eslint-define-config": "^1.14.0",
    "expect-type": "^0.15.0",
    "fast-glob": "^3.2.12",
    "hookable": "^5.4.2",
    "npm-run-all": "^4.1.5",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "turbo": "^1.7.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.28.1",
    "vue": "^3.2.45",
    "vue2": "npm:vue@^2.7.14",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "@algolia/client-search",
        "@yarnpkg/core"
      ]
    }
  }
}
