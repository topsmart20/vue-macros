{
  "name": "unplugin-vue-define-options",
  "version": "0.5.0",
  "description": "Add defineOptions support for Vue script-setup",
  "keywords": [
    "unplugin",
    "vue",
    "script-setup",
    "defineOptions"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-define-options.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "rm -fr dist && tsup && esno scripts/postbuild.ts",
    "dev": "MODE=dev tsup",
    "release": "standard-version",
    "lint": "eslint . --ext .vue,.ts --max-warnings 0"
  },
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-define-options/issues"
  },
  "homepage": "https://github.com/sxzz/unplugin-vue-define-options#readme",
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.0",
    "@vue/compiler-sfc": "^3.2.31",
    "unplugin": "^0.6.0"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@sxzz/eslint-config-prettier": "^2.1.0",
    "@sxzz/eslint-config-vue": "^2.1.0",
    "@types/node": "*",
    "@vitest/ui": "0.8.0",
    "eslint": "^8.12.0",
    "eslint-define-config": "^1.3.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "rollup": "^2.70.1",
    "standard-version": "^9.3.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "unplugin-vue": "2.0.0",
    "vite": "^2.8.6",
    "vitest": "0.8.0",
    "vue": "^3.2.31"
  },
  "engines": {
    "node": ">=12.20.0"
  }
}
